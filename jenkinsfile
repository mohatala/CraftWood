pipeline{
  agent any
  stages{
        stage('Build'){
            steps{
                bat 'mvn -Dmaven.test.skip=true install'
            }
        }
        stage('Test'){
              steps{
                  bat 'mvn test -Dtest=CommandeDAOTest'
                  bat 'mvn test -Dtest=ClientDAOTest'
                  bat 'mvn test -Dtest=ArticleDAOTest'
              }
        }
  }
}
